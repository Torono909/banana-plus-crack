{
  "schemaVersion": 1,
  "id": "bananaplus",
  "version": "${version}",
  "name": "Banana+",
  "description": "Banana+ addon for the Meteor Addon API.",
  "authors": [
    "Bennooo",
    "Necro"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bplusdevelopment.addons.AddModule"
    ]
  },
  "mixins": [
    "banana.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "255,193,0"
  },
  "depends": {
    "java": ">=16",
    "minecraft": ">=1.17",
    "meteor-client": ">0.4.4"
  }
}
